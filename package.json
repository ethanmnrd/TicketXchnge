  "license": "Apache",
  "scripts": {
    "clean": "rimraf ./server/frontend/static/dist/",
    "dev": "npm-run-all -p dev:start dev:wds",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node client/util/server.js",
    "dev:wds": "cross-env NODE_ENV=development webpack-dev-server  --progress --mode development --config webpack.config.dev.babel.js",
    "prod:analyze": "yarn clean && babel client/src -d lib --ignore .test.js && cross-env NODE_ENV=analyze webpack -p --progress --mode production --config webpack.config.analyze.babel.js",
    "prod:build": "yarn clean && babel client/src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress --mode production --config webpack.config.prod.babel.js --output ./server/frontend/static/dist/main.js",
    "lint": "eslint client webpack.config.dev.babel.js --ext .js,.jsx",
    "lint-fix": "prettier --single-quote --write \"{,client/**}/*.{js,jsx}\" && yarn lint --fix",
    "reinstall": "rm -rf node_modules/ && yarn install",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-flow-react-proptypes": "^24.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.82.0",
    "jest": "^23.6.0",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.0.2",
    "pm2": "^3.1.3",
    "postcss": "^7.0.4",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.23.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.2.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^4.1.3",
    "compression": "^1.6.2",
    "express": "^4.16.3",
    "lodash": "^4.17.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^4.1.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "reactstrap": "^6.5.0"
  }
}
